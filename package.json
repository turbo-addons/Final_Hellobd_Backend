{
    "name": "laradashboard",
    "private": true,
    "version": "0.9.2",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "build:all": "npm run build && npm run build:modules",
        "build:modules": "node scripts/build-modules.js",
        "build:ssr": "vite build && vite build --ssr",
        "build:release": "chmod +x ./scripts/build-release.sh && ./scripts/build-release.sh",
        "dev": "cross-env VITE_INCLUDE_MODULES=true vite",
        "dev:core": "vite",
        "format": "prettier --write resources/",
        "format:check": "prettier --check resources/",
        "lint": "eslint . --fix",
        "types": "tsc --noEmit",
        "commit": "cz",
        "prepare": "husky"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@eslint/js": "^9.19.0",
        "@tailwindcss/vite": "^4.0.15",
        "axios": "^1.11.0",
        "commitizen": "^4.3.1",
        "cross-env": "^10.1.0",
        "cz-conventional-changelog": "^3.0.1",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.0.1",
        "husky": "^9.1.7",
        "laravel-vite-plugin": "^2.0",
        "tailwindcss": "^4.0.15",
        "vite": "^7.2.7"
    },
    "dependencies": {
        "@alpinejs/focus": "^3.14.9",
        "@alpinejs/persist": "^3.14.9",
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@popperjs/core": "^2.11.8",
        "@vitejs/plugin-react": "^4.3.4",
        "alpinejs": "^3.14.9",
        "apexcharts": "^4.5.0",
        "bootstrap-icons": "^1.11.3",
        "dropzone": "^6.0.0-beta.2",
        "flatpickr": "^4.6.13",
        "fullcalendar": "^6.1.15",
        "iconify-icon": "^3.0.0",
        "jsvectormap": "^1.6.0",
        "playwright": "^1.57.0",
        "puppeteer": "^24.14.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "simple-datatables": "^10.0.0",
        "tailwind-merge": "^3.0.1",
        "tailwindcss": "^4.0.0",
        "tailwindcss-animate": "^1.0.7",
        "uuid": "^13.0.0",
        "vite": "^7.2.7"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "packageManager": "npm@^10.2.4",
    "overrides": {
        "tmp": "^0.2.4"
    }
}
